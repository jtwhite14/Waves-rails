<%= link_to 'buoys', buoys_path %> | <%= link_to 'sessions', wave_sessions_path %> 
<br />
<% @sessions.each do |session| %>
  <%= image_tag session.session_photo.feed.url %> <br />
  User: <%= session.wave.user.name %> <br />
  Rating: <%= session.rating %> <br />
  Comments: <%= session.notes %> <br />
<hr />     
<% end %>
  
<%= paginate @sessions %>
